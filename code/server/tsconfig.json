{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2016",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"suppressImplicitAnyIndexErrors": true,
		"sourceMap": false,
		"outDir": "app",
		"baseUrl": ".",
		"types": [],
		"paths": {
			"thinkjs": ["src/index"]
		}
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules", "**/*.spec.ts"]
}
